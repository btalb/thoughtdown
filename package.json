{
  "name": "thoughtdown",
  "version": "0.1.1",
  "description": "Transform thoughts into HTML (powered by Markdown)",
  "type": "module",
  "main": "./dist/index.cjs",
  "private": true,
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "rollup --config",
    "format": "prettier --write . && eslint --fix .",
    "start": "rollup --config --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btalb/thoughtdown.git"
  },
  "keywords": [
    "markdown",
    "d3",
    "html",
    "charts",
    "maths"
  ],
  "author": "Ben Talbot",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/btalb/thoughtdown/issues"
  },
  "homepage": "https://github.com/btalb/thoughtdown#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.4",
    "nodemon": "^2.0.4",
    "rollup": "^2.21.0"
  },
  "dependencies": {
    "d3": "^5.16.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "jsdom": "^16.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-highlightjs": "^3.4.0",
    "prettier": "^2.0.5",
    "seedrandom": "^3.0.5",
    "yaml": "^1.10.0"
  }
}
